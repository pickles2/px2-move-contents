{
    "name": "pickles2/px2-move-contents",
    "description": "Pickles 2 の既に制作済みのコンテンツの物理パスを変更(移動)します。",
    "keywords": ["pickles2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomoya Koyanagi",
            "email": "tomk79@gmail.com"
        }
    ],
    "scripts":{
        "test": [
            "php ./vendor/phpunit/phpunit/phpunit"
        ]
    } ,
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-mbstring": "*",
        "tomk79/filesystem": "^1.0.0"
    },
    "require-dev": {
        "pickles2/px-fw-2.x": "^2.0",
        "phpunit/phpunit": "^4.1.0"
    },
    "autoload": {
        "files": [
            "php/main.php",
            "php/move_content.php",
            "php/resolve_path.php",
            "php/px2agent.php",
            "php/utils.php",
            "php/simple_html_dom.php"
        ]
    },
    "extra": {
        "px2package": {
            "name": "tomk79/px2-move-contents",
            "type": "plugin"
        }
    }
}
