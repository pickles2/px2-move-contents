{
    "name": "tomk79/px2-move-contents",
    "description": "Pickles 2 は、静的で大きなウェブサイトを効率よく構築できる オープンソースのHTML生成ツールです。",
    "keywords": ["pickles2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomoya Koyanagi",
            "email": "tomk79@gmail.com"
        }
    ],
    "scripts":{
        "test": [
            "php ./vendor/phpunit/phpunit/phpunit"
        ]
    } ,
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-mbstring": "*",
        "tomk79/filesystem": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.1.0"
    },
    "autoload": {
        "files": [
            "php/main.php"
        ]
    },
    "extra": {
        "px2package": {
            "name": "tomk79/px2-move-contents",
            "type": "plugin"
        }
    }
}
